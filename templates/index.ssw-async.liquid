{% comment %}
/**************************************************
* DO NOT MODIFY THIS FILE!!!                      *
* IT WILL BE OVERWRITTEN BY UPDATES FROM GROWAVE! *
**************************************************/
{% endcomment %}
{% layout none %}
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="robots" content="noindex">
    <meta name="googlebot" content="noindex">
    <title></title>
</head>

<body>
<div id="ssw-async-html">
    <div id="ssw-discount-modal" class="ssw-modal ssw-fade" aria-hidden="true" role="modal">
        <div class="ssw-modal-dialog">
            <div class="ssw-modal-content">
                <div class="ssw-modal-body">
                    <button type="button" class="ssw-close" data-dismiss="ssw-modal" aria-hidden="true">&times;</button>
                    <p id="discount-instructions"> {{ shop.metafields.ssw.lang.your_discount_code }}</p>
                    <div class="h1" id="ssw-discount-code"></div>
                    <br />
                    <span id="discount-date"></span>
                </div>
            </div>
        </div>
    </div>
  {% capture ssw_helpers %}{% render 'ssw-helpers' %}{% endcapture %}
  {% unless ssw_helpers contains 'Liquid error' %}
    {{ ssw_helpers }}
  {% endunless %}
  {% capture socialshopwave_gplus %}{% render 'socialshopwave-gplus' %}{% endcapture %}
  {% unless socialshopwave_gplus contains 'Liquid error' %}
        {{ socialshopwave_gplus }}
  {% endunless %}
</div>
</body>
</html>